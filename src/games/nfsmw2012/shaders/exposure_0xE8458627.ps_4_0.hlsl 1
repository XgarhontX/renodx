// ---- Created with 3Dmigoto v1.3.16 on Tue Nov 11 02:11:30 2025

SamplerState DiffuseSampler_s : register(s0);
Texture2D<float4> DiffuseSampler : register(t0);


// 3Dmigoto declarations
#define cmp -
#include "../shared.h"


void main(
  float4 v0 : SV_Position0,
  float2 v1 : TEXCOORD0,
  out float4 o0 : SV_Target0)
{
  float4 r0,r1;
  uint4 bitmask, uiDest;
  float4 fDest;

  r0.xyzw = DiffuseSampler.Sample(DiffuseSampler_s, v1.xy).xyzw;
  r0.xyzw *= CUSTOM_BLOOM; //fools exposure to think bloom is brighter.
  r0.xyz = r0.www * r0.xyz;
  r0.xyz = float3(32,32,32) * r0.xyz;
  r0.xyz = exp2(-r0.xyz);
  r0.xyz = float3(1,1,1) + -r0.xyz;
  r0.xyz = max(float3(0,0,0), r0.xyz);
  r1.xyz = r0.xyz * float3(0.0707062855,0.0707062855,0.0707062855) + float3(-0.317853957,-0.317853957,-0.317853957);
  r1.xyz = r1.xyz * r0.xyz + float3(-0.0227105431,-0.0227105431,-0.0227105431);
  r0.xyz = sqrt(r0.xyz);
  r0.xyz = saturate(r0.xyz * float3(1.27169645,1.27169645,1.27169645) + r1.xyz);
  o0.xyzw = dot(r0.xyz, float3(0.298999995, 0.587000012, 0.114));
  return;
}